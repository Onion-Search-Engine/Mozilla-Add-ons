{
  "manifest_version": 2,
  "name": "Onion Search Engine",
  "version": "3.4",
  "description": "Extension to search on Onion Search Engine directly from your toolbar.",
  "author": "Onion Search Engine LLC",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Search on Onion Search Engine",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
